{
  "name": "redux-toolkit-swagger-gen",
  "version": "0.1.8",
  "description": "Redux Toolkit API client generator from Swagger/OpenAPI specifications",
  "main": "dist/src/index.js",
  "type": "commonjs",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "generate": "node ./bin/swagger-gen.js",
    "swagger-gen": "node ./bin/swagger-gen.js",
    "dev": "ts-node --files src/generate.ts",
    "start": "node dist/generate.js"
  },
  "bin": {
    "swagger-gen": "./bin/swagger-gen.js"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "github:apidevtools/json-schema-ref-parser",
    "@reduxjs/toolkit": "^2.4.0",
    "@types/react": "^19.0.10",
    "axios": "^1.8.3",
    "mustache": "^4.2.0",
    "yargs": "^17.7.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@types/node": "^16.11.7",
    "@types/yargs": "^17.0.32",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "redux",
    "redux-toolkit",
    "swagger",
    "openapi",
    "generator",
    "typescript",
    "api-client",
    "code-generator"
  ],
  "author": "Rodel Letran",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
