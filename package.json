{
  "name": "redux-toolkit-swagger-gen",
  "version": "0.1.13",
  "description": "Redux Toolkit API client generator from Swagger/OpenAPI specifications",
  "main": "dist/src/generate.js",
  "type": "commonjs",
  "types": "dist/src/generate.d.ts",
  "files": [
    "dist",
    "src",
    "bin",
    "README.md",
    "src/templates",
    "dist/src/templates"
  ],
  "scripts": {
    "build": "tsc && node scripts/copy-templates.js",
    "prepare": "npm run build",
    "generate": "node ./bin/swagger-gen.js",
    "swagger-gen": "node ./bin/swagger-gen.js",
    "swagger-gen:src": "ts-node --files src/generate.ts --url src/test/swagger-files/money-exchange.json --output src/api-money-exchange.json --clean --verbose",
    "swagger-gen:src2": "ts-node --files src/generate.ts --url src/test/swagger-files/swagger-snippet.json --output src/api-swagger-snippet.json --clean --verbose",
    "swagger-gen:dist": "node ./bin/swagger-gen.js --url src/test/swagger-files/identity.json --output src/api-identity --clean --verbose",
    "dev": "ts-node --files src/generate.ts",
    "start": "node dist/generate.js"
  },
  "bin": {
    "swagger-gen": "bin/swagger-gen.js"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "github:apidevtools/json-schema-ref-parser",
    "@reduxjs/toolkit": "^2.4.0",
    "@types/react": "^19.0.10",
    "axios": "^1.8.3",
    "mustache": "^4.2.0",
    "redux-persist": "^6.0.0",
    "yargs": "^17.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@types/node": "^16.11.7",
    "@types/yargs": "^17.0.32",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "redux",
    "redux-toolkit",
    "swagger",
    "openapi",
    "generator",
    "typescript",
    "api-client",
    "code-generator"
  ],
  "author": "Rodel Letran",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delletran/redux-toolkit-swagger-gen.git"
  },
  "bugs": {
    "url": "https://github.com/delletran/redux-toolkit-swagger-gen/issues"
  },
  "homepage": "https://github.com/delletran/redux-toolkit-swagger-gen#readme"
}
